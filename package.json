{
  "name": "vispackstream",
  "version": "1.0.1",
  "description": "Stream packaging test app",
  "main": "app.js",
  "bin": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --targets node16-linux-arm64 --output dist/vispackstream-arm64",
    "build:win": "pkg . --targets node16-win-x64 --output dist/vispackstream-win.exe",
    "build:all": "pkg . --targets node16-linux-arm64,node16-win-x64 --output dist/vispackstream"
  },
  "pkg": {
    "assets": [
      "www/**/*",
      "config/**/*",
      "lib/**/*"
    ],
    "targets": [
      "node16-linux-armv7"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "stream",
    "packaging",
    "node",
    "pkg"
  ],
  "author": "R",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "multer": "^1.4.5-lts.1",
    "os-utils": "^0.0.14"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
