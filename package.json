{
  "name": "vispackstream",
  "version": "1.0.1",
  "description": "Stream packaging test app",
  "main": "app.js",
  "bin": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --targets node18-linux-arm64 --output dist/vispackstream-arm64"
  },
  "pkg": {
    "assets": [
      "www/**/*",
      "config/**/*",
      "lib/**/*"
    ],
    "targets": [
      "node18-linux-arm64"
    ],
    "outputPath": "dist"
  },
  "keywords": ["stream", "packaging", "node", "pkg"],
  "author": "R",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "multer": "^2.0.2",
    "os-utils": "^0.0.14"
  }
}
